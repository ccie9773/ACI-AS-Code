                          bfd: true
                          as_override: true
          external_endpoint_groups:
            - name: external
              subnets:
                - prefix: 0.0.0.0/0
#                - prefix: 172.16.20.0/24
#                  export_route_control: true
                  
              contracts:
                providers:
                  - vrf_a_to_sda
